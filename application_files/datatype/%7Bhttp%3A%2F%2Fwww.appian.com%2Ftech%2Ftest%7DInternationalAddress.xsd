<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.appian.com/tech/test"
  xmlns:tns="http://www.appian.com/tech/test" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:complexType name="InternationalAddress">
    <xsd:sequence>
      <xsd:element form="qualified" name="streetAddress" type="xsd:string"/>
      <xsd:element form="qualified" name="aptOrSuite" type="xsd:string"/>
      <xsd:element form="qualified" name="city" type="xsd:string"/>
      <xsd:element form="qualified" name="stateOrProvince" type="xsd:string"/>
      <xsd:element form="qualified" name="postalCode" type="xsd:string"/>
      <xsd:element form="qualified" name="country" type="xsd:string"/>
      <xsd:element form="qualified" name="companyName" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
