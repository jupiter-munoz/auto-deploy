<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
  targetNamespace="https://jboss-verification.appianci.net/suite/types/"
  xmlns:tns="https://jboss-verification.appianci.net/suite/types/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:complexType name="HardcodedGridWithLinks">
    <xsd:annotation>
      <xsd:documentation><![CDATA[CDT containing fields intended to reflect hardcoded data for Grid Component with Links Tempo Report]]></xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="id" nillable="true" type="xsd:int"/>
      <xsd:element name="taskLabel" nillable="true" type="xsd:string"/>
      <xsd:element name="uri" nillable="true" type="xsd:string"/>
      <xsd:element name="linkLabel" nillable="true" type="xsd:string"/>
      <xsd:element name="date" nillable="true" type="xsd:string"/>
      <xsd:element name="amount" nillable="true" type="xsd:decimal"/>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
